<table class='custom-system-attributes'>
    <thead>
        <tr>
            <th>{{localize 'CSB.Attributes.Name'}}</th>
            <th>{{localize 'CSB.Attributes.Value'}}</th>
        </tr>
    </thead>
    <tbody>
        {{#each attributes}}
            <tr class='custom-system-hidden-attribute'>
                <td>
                    {{this.name}}
                </td>
                <td>
                    {{this.value}}
                </td>
            </tr>
        {{/each}}
    </tbody>
</table>